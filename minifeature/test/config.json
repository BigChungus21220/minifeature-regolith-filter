{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.4.json",
	"author": "Your name",
	"name": "Project name",
	"packs": {
		"behaviorPack": "./packs/BP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"filter_tester": {
				"url": "github.com/Bedrock-OSS/regolith-filters",
				"version": "1.0.0"
			},
			"minifeature": {
				"requirements": "../",
				"runWith": "nodejs",
				"script": "../parser.js"
			}
		},
		"formatVersion": "1.4.0",
		"profiles": {
			"default": {
				"export": {
					"bpName": "'BP'",
					"build": "standard",
					"readOnly": false,
					"target": "local"
				},
				"filters": [
					{
						"filter": "minifeature",
						"settings": {
							"features_directory": "./BP/features",
							"feature_rules_directory": "./BP/feature_rules",
							"minifeatures_directory": "./BP/minifeatures",
							"project_namespace": "minifeature",
							"namespaced_subfolders": true
						}
					},
					{
						"filter": "filter_tester",
						"settings": {
						  	"errors_stop_execution": false
						}
					}
				]
			}
		}
	}
}
{
  "namespace": "my_features",

  "my_rule": {
    "type": "rule",
    "places": {
      "type": "aggregate",
      "early_out": "first_success",
      "places": [
        "my_scatter",
        {
          "type": "block",
          "places_block": "minecraft:end_stone",
          "enforce_placement_rules": true,
          "enforce_survivability_rules": true
        },
        {
          "type": "block",
          "places_block": "minecraft:obsidian",
          "may_attach_to": {
            "top": "minecraft:air"
          },
          "enforce_placement_rules": true,
          "enforce_survivability_rules": true
        }
      ]
    },
    "conditions": {
      "placement_pass": "first_pass",
      "minecraft:biome_filter": [
        { "test": "has_biome_tag", "value": "birch" },
        {
          "any_of": [
            { "test": "has_biome_tag", "value": "spruce" },
            { "test": "has_biome_tag", "operator": "not", "value": "forest" }
          ]
        }
      ]
    },
    "distribution": {
      "iterations": "2",
      "x": 0,
      "y": "5",
      "z": {
        "distribution": "fixed_grid",
        "extent": [0, 2]
      }
    }
  },

  "my_scatter": {
    "type": "scatter",
    "places": {
      "type": "block",
      "places_block": "minecraft:stone",
      "enforce_placement_rules": true,
      "enforce_survivability_rules": true
    },
    "distribution": {
      "iterations": 10,
      "x": 0,
      "y": 0,
      "z": 0
    }
  }
}
